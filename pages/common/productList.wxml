<template name="product_list">
    <view class="products">
      <block wx:for="{{products}}" wx:key="{{index}}">
        <navigator url="details/details?id={{item.id}}" class="product-nav smallFoodContainer">
          <image mode="aspectFit" class="product-img smallImg" src="{{item.doc_image}}"/>
          <text class="product-name color">{{item.title}}</text>
          <text class="product-dec">{{item.intro}}</text>
          <text class="product-price bg_color">ï¿¥{{item.price}}</text>
          <view class="directAddCart" data-id="{{item.id}}" catchtap="directAddCart">
            <image src="/image/shop-cart.png"></image>
          </view>
        </navigator>
        
      </block>
    </view>
</template>